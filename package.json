{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build --preset=vercel_edge",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "supa-types": "npx supabase gen types typescript --project-id 'wmffmqkwpcjnmvrkemnx' --schema public > supabase/supabase.ts",
    "supabase:pull": "npx supabase db pull --schema auth,storage,public",
    "supabase:migration:diff": "npx supabase db diff --schema public",
    "supabase:backup": "npm run supabase:pull && npm run supa-types"
  },
  "dependencies": {
    "@nuxt/eslint": "^1.5.0",
    "@nuxt/fonts": "^0.11.4",
    "@nuxt/image": "^1.10.0",
