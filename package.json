    "@nuxtjs/color-mode": "^3.5.2",
    "@nuxtjs/supabase": "^1.5.3",
    "@tailwindcss/vite": "^4.1.11",
    "@unhead/vue": "^2.0.11",
    "@unlok-co/nuxt-stripe": "^5.0.0",
    "@vueuse/core": "^13.5.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dayjs-nuxt": "^2.1.11",
    "dotted-map": "^2.2.3",
    "eslint": "^9.30.1",
    "lucide-vue-next": "^0.525.0",
    "motion-v": "^1.4.0",
    "nuxt": "^3.17.6",
    "reka-ui": "^2.3.2",
    "shadcn-nuxt": "^2.2.0",
